{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "EmptyQueueWaitSeconds": 60,
    "JobProcessorOffTimeCheckSeconds": 60,
    "JobProcessorOptions": {
        "JobQueueName": "pcs-jobs",
