{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "JobProcessorOptions": {
        "JobQueueName": "pcs-jobs",
        "QueuePollTimeout": "00:01:00",
        "MaxJobRetries": 3,
        "QueueMessageInvisibilityTime": "00:01:00"
    },
    "KustoDatabase": "engineeringdata",
    "GitHubAuthentication": {
        "SaveTokens": true,
        "CallbackPath": "/signin/github",
        "UserAgentProduct": "",
        "ClaimsIssuer": "github"
    }
}
